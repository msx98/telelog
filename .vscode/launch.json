{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/Pipeline",
                "POSTGRES_HOST_OVERRIDE": "127.0.0.1",
		        "POSTGRES_PORT_OVERRIDE": "5200",
                "STOCK_DATA_PATH_OVERRIDE": "${workspaceFolder}/MessageStore/stockdata",
            }
        }
    ]
}
